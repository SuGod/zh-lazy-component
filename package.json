{
  "name": "zh-lazy-component",
  "version": "1.0.0",
  "private": false,
  "description": "组件懒加载",
  "author": {
    "name": "Zeddy",
    "url": "http://zeddy.site"
  },
  "license": "MIT",
  "homepage": "https://github.com/SuGod/zh-lazy-component",
  "repository": {
    "type": "git",
    "url": "https://github.com/SuGod/zh-lazy-component.git"
  },
  "keywords": [
    "vue.js",
    "vue-plugin",
    "vue-component",
    "lazy-load"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lib": "vue-cli-service build --target lib --name zhLazyComponent --dest lib ./src/index.js",
    "lint": "vue-cli-service lint",
    "prepublish": "npm run lib"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^9.4.3",
    "vue-template-compiler": "^2.6.10",
    "core-js": "^3.4.3",
    "vue": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
